package step1;

public class Test {

	public static void main(String[] args) {
		
		int pageNo = 1;
		
		int rowIndex = 754;
		int rowIf = 10;
		int pageIf = 5;
		
		// rowMax = 
		int rowEnd = pageNo*rowIf;
		int rowStart = rowEnd-(rowIf-1); // 10 - 10-1 .... 30 - 10-1
		if(rowEnd>rowIndex) {
			rowEnd = rowIndex;
		}
		//ÃÑ ÆäÀÌÁö ¼³Á¤
		int pageEnd = rowIndex/pageIf+(rowIndex%pageIf>0?1:0); // 754/5 + 1
		if(pageNo>pageEnd) {
			pageNo = pageEnd;
		}
	}

}
